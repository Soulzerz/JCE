<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="main.java.modelos.Partido" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
      <id name="Id" column="PartidoId">
          <generator class="native"/>
      </id>
      <property name="Nombre" column="Nombre"/>
      <property name="totalVotos" column="total_Votos"/>
  </class>
</hibernate-mapping>
